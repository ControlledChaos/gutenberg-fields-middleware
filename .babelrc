{
	"presets": [
		"react",
		[ "env", {
			"modules": false,
			"targets": {
				"browsers": [
					"last 2 Chrome versions",
					"last 2 Firefox versions",
					"last 2 Safari versions",
					"last 2 iOS versions",
					"last 1 Android version",
					"last 1 ChromeAndroid version",
					"ie 11"
				]
			}
		} ]
	],
	"env": {
		"test": {
			"presets": [["env"], "react"]
		}
	},
	"plugins": [
	  	"transform-object-rest-spread",
		[ "transform-react-jsx", {
			"pragma": "wp.element.createElement"
		} ]
	]
}
